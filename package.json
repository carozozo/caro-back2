{
  "name": "caro-back2",
  "version": "1.0.0",
  "description": "處理快送和社區幫雜務事項",
  "author": "Caro",
  "private": true,
  "engines": {
    "node": "14.16.0"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.js; npm run log",
    "start:prod": "pm2 start ecosystem.config.js --env prod; npm run log",
    "deploy": "bash docker/deploy.sh",
    "cron.exec": "NODE_ENV=dev NODE_PATH=. JOB_NAME=$J ITEM_NAME=$I node exec_cron",
    "stacker.exec": "NODE_ENV=dev NODE_PATH=. GROUP_NAME=$G TASK_NAME=$T ENTRY_NAME=$E node exec_stacker",
    "log": "pm2 logs caro-back2 --raw --lines 500",
    "log:prod": "ssh caro-back2 'sudo docker exec -i caro-back2 bash -c \"npm run log\"'",
    "sample:main": "TARGET=$T PROJECT=main node ./sample.js",
    "sample:proj1": "TARGET=$T PROJECT=proj1 node ./sample.js",
    "sample.delete:main": "TARGET=$T PROJECT=main IS_DELETE=true node ./sample.js",
    "sample.delete:proj1": "TARGET=$T PROJECT=proj1 IS_DELETE=true node ./sample.js",
    "build": "bash docker/build.sh",
    "push": "bash docker/push.sh",
    "up": "bash docker/up.sh"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^5.5.0",
    "aes-js": "^3.1.2",
    "aws-sdk": "^2.791.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-mongodb-session": "^2.4.1",
    "csv-parse": "^4.14.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify-es": "^2.0.0",
    "gunzip-maybe": "^1.4.2",
    "helmet": "^4.2.0",
    "iconv": "^3.0.0",
    "jschardet": "^2.2.1",
    "jsftp": "^2.1.3",
    "json2csv": "^5.0.4",
    "lodash": "^4.17.20",
    "minizip-asm.js": "^1.0.10",
    "moment": "^2.29.1",
    "mongodb": "^3.6.3",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.16",
    "progress": "^2.0.3",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "tar-stream": "^2.1.4",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {}
}
